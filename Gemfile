source 'https://rubygems.org'
# 使用 GitHub Pages 官方维护的版本集（包含 jekyll, jemoji, jekyll-paginate 等）
gem "github-pages", group: :jekyll_plugins
gem 'jekyll-paginate'
gem "jemoji"
gem "jekyll", "~> 4.0"
gem "rake"

gem "webrick", "~> 1.8"
# 如果你需要特定的时区支持（Windows常见报错修复）
gem "tzinfo-data", platforms: [:mingw, :mswin, :x64_mingw, :jruby]
